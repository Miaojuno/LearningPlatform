<#import "/main/macros.ftl" as macros>
<#assign importCss=[]>
<#assign importJs=[]>
<@macros.navhead importJs=importJs importCss=importCss></@macros.navhead>

<div class="main-contain" style="margin-top: 1rem">
    <div>
        <p class="nop-test">对8起，你没有这个权限嗷~</p>
    </div>
    <img class='carousel-img rounded nop-img' src="data:image/jpeg;base64,">
</div>

<style>
    .nop-img{
        max-width: 20rem;
        max-height: 20rem;
    }
    .nop-test{
        font-size: 2rem;
    }
</style>

<script>
    $(".main-contain").css("height", $(window).height() * 0.8)
    if (!/(iPhone|iPad|iPod|iOS|Android)/i.test(navigator.userAgent)) { //非移动端
        $(".nop-test").css("top", $(window).height() * 0.4)
        $(".nop-test").css("right", $(window).width() * 0.5)
        $(".nop-test").css("position", "absolute")
        $(".nop-img").css("top", $(window).height() * 0.2)
        $(".nop-img").css("left", $(window).width() * 0.5)
        $(".nop-img").css("position", "absolute")
    }

</script>